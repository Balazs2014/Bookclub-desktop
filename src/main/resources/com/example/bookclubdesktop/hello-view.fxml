<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="650.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11" fx:controller="com.example.bookclubdesktop.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button mnemonicParsing="false" onAction="#onTiltasClick" text="Tiltás / Tiltás visszavonása" />
      <TableView fx:id="tableView" prefHeight="258.0" prefWidth="610.0">
        <columns>
          <TableColumn fx:id="colName" minWidth="80.0" text="Név" />
          <TableColumn fx:id="colGender" prefWidth="10.0" text="Nem" />
            <TableColumn fx:id="colBirth_date" prefWidth="10.0" text="Születésnap" />
            <TableColumn fx:id="colBanned" prefWidth="10.0" text="Tiltva" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
